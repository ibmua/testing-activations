layer {
  # bottom: 		"minout"
  bottom: 		"maxout1"
  type: 																		"InnerProduct"
  name: 		"predicted_out"
  top: 			"predicted_out"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 							1
    weight_filler {
      type: "xavier"
		std: 0.01

    }
    bias_filler {
      type: "constant"
      value: 0.1
    }
  }
}




layer {
  name: 	"loss"
  type:	 	"EuclideanLoss"
  bottom:	"predicted_out"
  bottom:	"out"
  top: 		"loss"
}