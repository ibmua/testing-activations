layer {
  bottom: 						"slice5A"
  type: 													"InnerProduct"
  name: 						"group_A"
  top: 							"group_A"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
	num_output:									1
    weight_filler {
		type: "xavier"
		std: 0.2
    }
    bias_filler {
      type: "constant"
      value: 0.01
    }
  }
}



layer {
  bottom: 						"slice5B"
  type: 													"InnerProduct"
  name: 						"group_B"
  top: 							"group_B"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
	num_output:									1
    weight_filler {
		type: "xavier"
		std: 0.2
    }
    bias_filler {
      type: "constant"
      value: 0.01
    }
  }
}



