layer {
  include {
    phase: TRAIN
  }

  name: "out"
  type: "Input"

  top: "out"
  input_param {
    shape {
      #dim: 64
      dim: 1
      dim: 1
    }
  }
}
layer {
  include {
    phase: TEST
  }

  name: "out"
  type: "Input"

  top: "out"
  input_param {
    shape {
      #dim: 64
      dim: 1
      dim: 1
    }
  }
}

layer {
  include {
    phase: TRAIN
  }

  name: "in"
  type: "Input"
  top: "in"
  
  
  input_param {
    shape {
      #dim: 64
      dim: 1
      dim: 1
    }
  }

  

}
layer {
  include {
    phase: TEST
  }

  name: "in"
  type: "Input"
  top: "in"


  input_param {
    shape {
      #dim: 64
	  dim: 1
      dim: 1
    }
  }
  
}










layer {
  bottom: 		"in"
  type: 													"InnerProduct"
  name: 						"in_to_pyramid"
  top: 							"in_to_pyramid"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
	num_output:									6
    weight_filler {
		type: "xavier"
		std: 0.2
    }
    bias_filler {
      type: "constant"
      value: 0.01
		# type: "xavier"
		# std: 0.01
    }
  }
}


layer {
  bottom: 				"in_to_pyramid"
  top: 					"in_to_pyramid"
  name: 				"in_to_pyramid_rectifier"
  type: 													"PReLU"
}




layer {
  bottom:	"in_to_pyramid"
  name: 	"slice5"
  type: 			'Slice'
  top: 		"slice5A"
  top: 		"slice5B"
  slice_param {
    slice_dim:		1
	
    slice_point:	3
  }
}



