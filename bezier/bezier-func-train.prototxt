layer {
  bottom: "group_A"
  bottom: "group_B"
  type: "Concat"
  concat_param {
    axis: 1
  }
  name:	"concat"
  top:	"concat"
}



layer {
  bottom: 				"concat"
  top: 					"concat"
  name: 				"concat_rectifier"
  type: 													"Softmax"
}




layer {
  bottom: 						"concat"
  type: 													"InnerProduct"
  name: 						"maxout1"
  top: 							"maxout1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
	num_output:									1
    weight_filler {
		type: "xavier"
		std: 0.2
    }
    bias_filler {
      type: "constant"
      value: 0.01
    }
  }
}





